{
  "name": "RNTemplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "lint": "eslint .",
    "test": "jest",
    "android:dev": "ENVFILE=.env.development react-native run-android",
    "android:prod": "ENVFILE=.env.production react-native run-android --variant=release",
    "ios:dev": "ENVFILE=.env.development react-native run-ios --scheme RNTemplate --configuration Debug",
    "ios:prod": "ENVFILE=.env.production react-native run-ios --scheme RNTemplate --configuration Release",
    "build:android:dev": "cd android && ENVFILE=../.env.development ./gradlew assembleDebug && cd ..",
    "build:android:prod": "cd android && ENVFILE=../.env.production ./gradlew assembleRelease && cd ..",
    "build:ios:dev": "cd ios && xcodebuild -workspace RNTemplate.xcworkspace -scheme RNTemplate -configuration Debug -sdk iphonesimulator -derivedDataPath build && cd ..",
    "build:ios:prod": "cd ios && xcodebuild -workspace RNTemplate.xcworkspace -scheme RNTemplate -configuration Release -sdk iphoneos -derivedDataPath build && cd ..",
    "fastlane:android": "cd android && bundle exec fastlane deploy",
    "fastlane:ios": "cd ios && bundle exec fastlane deploy"
  },
  "dependencies": {
    "@react-native/new-app-screen": "0.82.1",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "react": "19.1.1",
    "react-native": "0.82.1",
    "react-native-config": "^1.5.3",
    "react-native-reanimated": "^4.1.5",
    "react-native-safe-area-context": "^5.5.2",
    "react-native-screens": "^4.18.0",
    "react-native-worklets": "^0.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "20.0.0",
    "@react-native-community/cli-platform-android": "20.0.0",
    "@react-native-community/cli-platform-ios": "20.0.0",
    "@react-native/babel-preset": "0.82.1",
    "@react-native/eslint-config": "0.82.1",
    "@react-native/metro-config": "0.82.1",
    "@react-native/typescript-config": "0.82.1",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.1.1",
    "@types/react-test-renderer": "^19.1.0",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "19.1.1",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=20"
  }
}
